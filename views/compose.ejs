<%- include("partials/header") %>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" name="composeTitle" class="form-control" id="title" aria-describedby="Title" placeholder="Enter Post Title">
  </div>
  <div class="form-group">
    <label for="post">Posts</label>
    <textarea class="form-control" name="composePost" id="post" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") %>
